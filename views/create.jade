.create-done(style="display: none; text-align: center;")
	.create-info
		i(style="font-size: 210px; color: #1abc9c;").fa.fa-check-circle
		h2(style="margin-bottom: -5px;")="Thanks, "
			span#name-text
			="!"
		div(style="margin-bottom: 15px;")="You're finished! Go ahead and sign in to check out what Rodney can do."
	button.btn.btn-outline.width-50#login-from-create="Log in"